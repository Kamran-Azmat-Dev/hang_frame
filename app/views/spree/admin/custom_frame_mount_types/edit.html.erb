<% content_for :page_title do %>
  <%= link_to Spree.t(:custom_frame_mount_types), spree.admin_custom_frame_additionals_url %> /
  <%= @custom_frame_mount_type.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @custom_frame_mount_type } %>

<%= form_for [:admin, @custom_frame_mount_type] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>

    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
