<% content_for :page_title do %>
  <%= link_to Spree.t(:frame_additionals), spree.admin_custom_frame_additionals_url %> /
  <%= Spree.t(:new_frame_additional) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @custom_frame_additional } %>

<%= form_for [:admin, @custom_frame_additional] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
