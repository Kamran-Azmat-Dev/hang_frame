<% content_for :page_title do %>
  <%= link_to Spree.t(:original_art_types), spree.admin_original_art_types_url %> /
  <%= Spree.t(:new_original_art_type) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @original_art_type } %>

<%= form_for [:admin, @original_art_type] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
