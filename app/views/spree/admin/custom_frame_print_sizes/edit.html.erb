<% content_for :page_title do %>
  <%= link_to Spree.t(:frame_print_sizes), spree.admin_custom_frame_print_sizes_url %> /
  <%= @custom_frame_print_size.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @custom_frame_print_size } %>

<%= form_for [:admin, @custom_frame_print_size] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>

    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
