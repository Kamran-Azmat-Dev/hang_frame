<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @frame_mount_preview_image } %>

<%= form_for @custom_frame_moulding_type_price, url: admin_custom_frame_print_size_custom_frame_moulding_type_price_url(@print_size, @custom_frame_moulding_type_price), as: :custom_frame_moulding_type_price, html: { multipart: true } do |f| %>
  <div data-hook="edit_image" class="card mb-3">
    <div class="card-body">
      <div class="form-group">
        <div class="col-12 col-lg-9">
          <%= render partial: 'form', locals: { f: f } %>
        </div>

        <div class="form-actions" data-hook="buttons">
          <%= button Spree.t('actions.update'), 'save.svg' %>
          <span class="or"><%= Spree.t(:or) %></span>
          <%= button_link_to Spree.t('actions.cancel'), spree.admin_custom_frame_print_size_custom_frame_moulding_type_prices_url(@print_size), id: 'cancel_link', icon: 'cancel.svg' %>
        </div>
      </div>
    </div>
  </div>
<% end %>
