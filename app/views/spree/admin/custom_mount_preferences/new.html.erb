<% content_for :page_title do %>
  <%= link_to Spree.t(:custom_mount_preference), spree.admin_custom_mount_preferences_url %> /
  <%= Spree.t(:new_custom_mount_preference) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @custom_mount_preference } %>

<%= form_for @custom_mount_preference, url: admin_custom_mount_preferences_url do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
